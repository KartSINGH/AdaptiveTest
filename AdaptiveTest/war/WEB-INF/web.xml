		<servlet-name>TestPage</servlet-name>
		<jsp-file>/test.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestPage</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>Index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
